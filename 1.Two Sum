class Solution {
    public int[] twoSum(int[] nums, int target) {
        int brr[]=new int[2];
        int i=0,j=0;
        for(i=0;i<nums.length-1;i++)
        {
            for(j=1+i;j<nums.length;j++)
            {
                int sum=nums[i] + nums[j];
                if(sum==target)
                 {
                brr[0]=i;
                brr[1]=j;
                break;
                 }
            }
        }
        return brr;
    }
}
